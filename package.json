{
  "name": "swdc-vscode",
  "displayName": "Code Time",
  "version": "2.2.3",
  "publisher": "softwaredotcom",
  "description": "Code Time is an open source plugin that provides programming metrics right in Visual Studio Code.",
  "author": {
    "name": "Software.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "icon": "images/app-icon-blue.png",
  "galleryBanner": {
    "color": "#384356",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swdotcom/swdc-vscode"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "time tracking",
    "productivity",
    "timer",
    "git",
    "spotify"
  ],
  "activationEvents": [
    "*"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "engines": {
    "vscode": "^1.35.0"
  },
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./",
    "tests": "mocha --require ts-node/register test/runTest.ts"
  },
  "contributes": {
    "commands": [
      {
        "command": "codetime.softwarePaletteMenu",
        "title": "Code Time: Menu options"
      },
      {
        "command": "cloud9.createTeam",
        "title": "Cloud9: Create a team"
      },
      {
        "command": "cloud9.joinTeam",
        "title": "Cloud9: Join a team"
      },
      {
        "command": "cloud9.leaderboard",
        "title": "Cloud9: Leaderboard"
      },
      {
        "command": "cloud9.debugClearUserId",
        "title": "Cloud9: Debug clear User Id"
      },
      {
        "command": "codetime.howardPush",
        "title": "push me"
      },
      {
        "command": "codetime.codeTimeMetrics",
        "title": "Code Time: View summary"
      },
      {
        "command": "codetime.softwareKpmDashboard",
        "title": "Code Time: See advanced metrics"
      },
      {
        "command": "codetime.displayReadme",
        "title": "Code Time: Learn more"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Code Time",
      "properties": {
        "showGitMetrics": {
          "type": "boolean",
          "default": false,
          "description": "Git metrics"
        },
        "toggleFileEventLogging": {
          "type": "boolean",
          "default": false,
          "description": "Enable file event logging"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "code-time",
          "title": "Code Time",
          "icon": "resources/dark/paw-outlined.svg"
        }
      ]
    },
    "views": {
      "code-time": [
        {
          "id": "ct-menu-tree",
          "name": "MENU"
        },
        {
          "id": "ct-metrics-tree",
          "name": "DAILY METRICS"
        },
        {
          "id": "ct-team-tree",
          "name": "CONTRIBUTORS"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.6",
    "@types/node": "^13.13.5",
    "@types/vscode": "^1.45.1",
    "vscode-test": "^1.3.0",
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mocha": "^7.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "vsce": "^1.75.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@slack/web-api": "^5.8.0",
    "aws-sdk": "^2.470.0",
    "axios": "^0.19.0",
    "clipboardy": "^2.1.0",
    "firebase": "^7.14.3",
    "husky": "^4.2.5",
    "moment-timezone": "^0.5.25",
    "node-cache": "^5.1.0",
    "numeral": "^2.0.6",
    "open": "^6.4.0",
    "require": "^2.4.20",
    "rxjs": "^6.5.5",
    "unique-names-generator": "^4.2.0",
    "vsls": "0.3.1291",
    "yarn": "^1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
